	<!-- 中文乱码处理 -->
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<package name="default1" namespace="/" extends="struts-default">
		<!-- 默认首起项配置 <default-action-ref name="findPage" /> -->
		<!-- ssm框架集合时,class的值是spring配置文件的实例化action的bean的id值 -->
		<!-- 1.查询所有 -->
		<action name="findAll" class="com.csp.page.action.DeptAction"
			method="findAll">
			<result name="success">/page/index.jsp</result>
			<result name="error">fail.jsp</result>
		</action>

		<!-- 1.查询所有 -->
		<action name="findPage" class="com.csp.page.action.DeptAction"
			method="findPage">
			<result name="success">/page/index.jsp</result>
			<result name="error">fail.jsp</result>
		</action>

		<!-- 2.根据id查询 -->
		<action name="findById" class="com.csp.page.action.DeptAction"
			method="findById">
			<result name="success">update.jsp</result>
			<!-- 重定向到action -->
			<result name="error" type="redirectAction">findAll</result>
		</action>

		<!-- 3.添加 -->
		<action name="save" class="com.csp.page.action.DeptAction"
			method="save">
			<!-- 重定向到action -->
			<result name="success" type="redirectAction">findAll</result>
			<result name="error">insert.jsp</result>
		</action>

		<!-- 4.修改 -->
		<action name="update" class="com.csp.page.action.DeptAction"
			method="update">
			<!-- 重定向到action -->
			<result name="success" type="redirectAction">findAll</result>
			<result name="error">fail.jsp</result>
		</action>

		<!-- 5.删除 -->
		<action name="delete" class="com.csp.page.action.DeptAction"
			method="delete">
			<!-- 重定向到action -->
			<result name="success" type="redirectAction">findPage</result>
			<result name="error">fail.jsp</result>
		</action>
	</package>
