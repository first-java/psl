<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jn.bin_record.file.dao.BinRecordDao">

	<!-- 自定义结果集,可以将sql语句查询的Resultset结果集与实体类一一对应 -->
	<resultMap type="com.jn.bin_record.entity.BinRecord" id="myPsp">
		<!-- 其他列映射都是用result -->
		<result column="VOLUME" property="volume" javaType="java.lang.String"
			jdbcType="VARCHAR" />
		<result column="REMARK" property="remark" javaType="java.lang.String"
			jdbcType="VARCHAR" />

	</resultMap>
	<!-- 上传 -->
	<insert id="save" parameterType="com.jn.bin_record.entity.BinRecord">
		insert into
		SYS_PSL.WAFER_BIN_RECORD
		values(#{volume,jdbcType=VARCHAR},#{remark,jdbcType=VARCHAR})
	</insert>
</mapper>