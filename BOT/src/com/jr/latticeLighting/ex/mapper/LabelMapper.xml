<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jr.latticeLighting.ex.dao.LabelDao">

	<!-- 自定义结果集,可以将sql语句查询的Resultset结果集与实体类一一对应 -->
	<resultMap type="com.jr.latticeLighting.entity.Label" id="label">
		<!-- 用于主键的映射 -->
		<!-- <id column="id" property="id" javaType="int" /> -->
		<result column="ORDERID" property="orderId" javaType="java.lang.String"
			jdbcType="VARCHAR" />
		<result column="materialcode" property="materialCode" javaType="java.lang.String"
			jdbcType="VARCHAR" />
		<result column="RA" property="ra" javaType="java.lang.String"
			jdbcType="VARCHAR" />
		<result column="CCT" property="cct" javaType="java.lang.String"
			jdbcType="VARCHAR" />
		<result column="luminance" property="luminance" javaType="java.lang.String"
			jdbcType="VARCHAR" />
		<result column="PRODUCT" property="product" javaType="java.lang.String"
			jdbcType="VARCHAR" />

	</resultMap>
	<!-- 针对于不定向条件的查询，关键在于SQL语句的拼接，根据当前实际的参数来拼接SQL -->

	<insert id="save">
		insert into
		SYS_PSL.LATTICELIGHTINGLABEL(orderid,materialcode,ra,cct,luminance,product)
		values(#{orderId,jdbcType=VARCHAR},#{materialCode,jdbcType=VARCHAR},#{ra,jdbcType=VARCHAR},
		#{cct,jdbcType=VARCHAR},#{luminance,jdbcType=VARCHAR},#{product,jdbcType=VARCHAR})
	</insert>
</mapper>